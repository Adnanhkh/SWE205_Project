<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ManagerListController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Text layoutX="54.0" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Manager Control Panel">
               <font>
                  <Font name="System Bold" size="28.0" />
               </font>
            </Text>
            <Button fx:id="addCarbt" layoutX="237.0" layoutY="140.0" mnemonicParsing="false" onAction="#goToAddCar" prefHeight="31.0" prefWidth="164.0" text="Add Car">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="removeCarbt" layoutX="238.0" layoutY="193.0" mnemonicParsing="false" onAction="#goToRemoveCar" prefHeight="31.0" prefWidth="164.0" text="Reomve Car">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="editCarbt" layoutX="237.0" layoutY="242.0" mnemonicParsing="false" onAction="#goToEditCar" text="Edit Car Information">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="logoutbt" layoutX="54.0" layoutY="341.0" mnemonicParsing="false" onAction="#logout" text="LogOut" textFill="RED">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
         </children>
    </AnchorPane>
  </children>
</VBox>
