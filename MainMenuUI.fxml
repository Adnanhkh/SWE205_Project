<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainMenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Text fill="#bd2c2c" layoutX="219.0" layoutY="114.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome Again!" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Text>
            <Button fx:id="carListbt" layoutX="228.0" layoutY="187.0" mnemonicParsing="false" onAction="#goToCarList" prefHeight="26.0" prefWidth="183.0" text="The Cars List" />
            <Button fx:id="managebt" layoutX="228.0" layoutY="234.0" mnemonicParsing="false" onAction="#goToManage" prefHeight="26.0" prefWidth="183.0" text="Manage My Reservations" />
            <Label layoutX="219.0" layoutY="124.0" text="Choose how could we serve you" textFill="#4901d7">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Button fx:id="statusbt" layoutX="228.0" layoutY="285.0" mnemonicParsing="false" onAction="#goToStatus" prefHeight="26.0" prefWidth="183.0" text="Reservation Status" />
            <Button fx:id="logoutbt" layoutX="14.0" layoutY="341.0" mnemonicParsing="false" onAction="#logout" text="LogOut" textFill="#d70000">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
