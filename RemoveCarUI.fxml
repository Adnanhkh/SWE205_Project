<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CarAddRemoveController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="mainMenuBt" layoutX="47.0" layoutY="340.0" mnemonicParsing="false" onAction="#backToMenu" text="Back To Main Menu">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="removeBt" layoutX="465.0" layoutY="340.0" mnemonicParsing="false" onAction="#removeCar" text="Remove the Car">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Text layoutX="130.0" layoutY="122.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter the ID of the Car you want to remove" textAlignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <TextField fx:id="carIDtxt" layoutX="218.0" layoutY="147.0" prefHeight="26.0" prefWidth="204.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
